
{
  "name": "Biom3d-Workflow-Omero",
  "description": "3D segmentation using Biom3d",
  "container-image": {
    "image": "samisafarbati/biom3d",
    "type": "singularity"
  },
  "command-line": "python wrapper.py IMG_DIR MSK_DIR NUM_CLASSES",
  "inputs": [
 
    {
      "id": "img_dir",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Image Directory",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    },
    {
      "id": "msk_dir",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Mask Directory",
      "set-by-server": false,
      "optional": false,
      "type": "String"
    },
    {
      "id": "num_classes",
      "value-key": "@ID",
      "command-line-flag": "--@id",
      "name": "Number of classes ",
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    }
  ],

  
  "schema-version": "cytomine-0.1"

}
